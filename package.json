{
  "name": "antd-vue-admin",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "liam",
    "email": "laim.wang@qq.com",
    "url": "https://github.com/liamwang"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint --ext .ts,.tsx ./src",
    "lint:fix": "tsc --noEmit && eslint --fix --cache --ext .ts,.tsx ./src",
    "lint:lint-staged": "lint-staged",
    "clean": "rimraf dist node_modules pnpm-lock.yaml yarn.lock package.lock.json .eslintcache",
    "clean:cache": "rimraf node_modules/.cache/ node_modules/.vite .eslintcache"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run lint",
    "**/*.{tsx,ts,less,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^3.0.0-beta.9",
    "vue": "^3.2.30",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.4",
    "@vue/compiler-sfc": "^3.2.30",
    "consola": "^2.15.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "vite": "^2.8.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^0.31.2"
  }
}
